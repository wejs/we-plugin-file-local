{
  "name": "we-plugin-file-local",
  "description": "We.js local storage plugin, save files to one folder inside the host",
  "version": "1.2.0",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js"
  },
  "dependencies": {
    "gm": "^1.23.0",
    "mkdirp": "^0.5.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "express-mysql-session": "^1.2.2",
    "fs-extra": "^4.0.1",
    "mocha": "^3.5.0",
    "mysql": "^2.14.1",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0",
    "we-core": "wejs/we-core",
    "we-plugin-acl": "wejs/we-plugin-acl",
    "we-plugin-file": "wejs/we-plugin-file",
    "we-plugin-user": "wejs/we-plugin-user",
    "we-test-tools": "wejs/we-test-tools"
  },
  "keywords": [
    "wejs-plugin",
    "file",
    "storage"
  ],
  "files": [
    "files/public",
    "lib",
    "locales",
    "server",
    "install.js",
    "plugin.js"
  ],
  "repository": "wejs/we-plugin-file-local",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "wejs": {
    "dependencies": {
      "we-plugin-file": true
    },
    "plugins": {
      "we-plugin-file": true,
      "we-plugin-acl": true,
      "we-plugin-user": true
    },
    "devPlugins": {}
  }
}
